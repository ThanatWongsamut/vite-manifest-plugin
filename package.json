{
  "name": "vite-manifest-plugin",
  "version": "1.1.0",
  "description": "This vite plugin will add publich path or url to the vite generated manifest",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache . --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm clean && pnpm build && pnpm test:run",
    "release": "pnpm version patch && git push && git push --tags"
  },
  "keywords": [
    "vite-plugin",
    "vite plugin",
    "module federation"
  ],
  "author": "thanat.wongamut",
  "license": "ISC",
  "dependencies": {
    "typescript": "^5.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThanatWongsamut/vite-manifest-plugin.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^20.14.8",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "^9.5.0",
    "tsup": "^8.1.0",
    "vite": "^5.3.1",
    "vitest": "^3.2.4"
  }
}
